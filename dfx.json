{
    "canisters": {
        "root": {
            "type": "custom",
            "candid": ".dfx/local/canisters/root/root.did",
            "build": "bash -c 'scripts/app/build.sh root'",
            "wasm": ".dfx/local/canisters/root/root.wasm",
            "shrink": true,
            "gzip": true,
            "metadata": [
                {
                    "name": "candid:service"
                }
            ],
            "dependencies": [
                "example"
            ]
        },
        "example": {
            "type": "custom",
            "candid": ".dfx/local/canisters/example/example.did",
            "build": "bash -c 'scripts/app/build.sh example'",
            "wasm": ".dfx/local/canisters/example/example.wasm",
            "shrink": true,
            "gzip": true,
            "metadata": [
                {
                    "name": "candid:service"
                }
            ]
        }
    },
    "networks": {
        "staging": {
            "providers": [
                "https://icp0.io"
            ],
            "type": "persistent"
        },
        "mainnet": {
            "providers": [
                "https://icp0.io"
            ],
            "type": "persistent"
        }
    },
    "defaults": {
        "build": {
            "output": "canisters/"
        }
    },
    "metadata": [
        {
            "name": "candid:service"
        }
    ],
    "version": 1
}
